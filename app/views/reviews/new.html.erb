<%= form_with model: @review, url: restaurant_reviews_path(@restaurant.id) do |form| %>

    <%= form.collection_select :restaurant_id, @restaurants, :id, :name %> 
    <%= form.text_area :comment %>
    <%= form.number_field :rating, value: 1, min: 1, max: 5 %>

    <%= form.submit %>

<% end %>